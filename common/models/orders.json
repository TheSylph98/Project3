{
  "name": "Order",
  "plural": "orders",
  "base": "Model",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true,
    "mydb": {
      "table": "orders"
    }
  },
  "properties": {
    "uid": {
      "type": "String",
      "required": true,
      "index": { "unique": true }
    },
    "userId": {
      "type": "number",
      "required": true
    },
    "bookId": {
      "type": ["number"],
      "required": true
    },
    "quantity": {
      "type": ["number"],
      "required": true
    },
    "price": {
      "type": ["number"],
      "required": true
    },
    "total": {
      "type": "number",
      "required": true
    },
    "promotionId": {
      "type": "number",
      "required": false
    },
    "createdAt": {
      "type": "date",
      "required": true
    },
    "paymentMethod": {
      "type": ["string"],
      "required": true
    },
    "addressShip": {
      "type": "string",
      "required": false
    },
    "status": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "userId"
    },
    "book": {
      "type": "belongsTo",
      "model": "Book",
      "foreignKey": "bookId"
    },
    "promotion": {
      "type": "belongsTo",
      "model": "Promotion",
      "foreignKey": "promotionId"
    }
  },
  "acls": [],
  "methods": {}
}
