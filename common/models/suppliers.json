{
  "name": "Supplier",
  "plural": "suppliers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
  	"validateUpsert": true,
    "strictObjectIDCoercion": true,
    "mydb": {
      "table": "suppliers"
    }
  },
  "properties": {
    "uid": {
      "type": "String",
      "required": true,
      "index": { "unique": true }
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true 
    },
    "address": {
      "type": "string",
      "required": false
      
    },
    "phone": {
      "type": "string",
      "required": false     
    },
    "createdAt": {
      "type": "Date",
      "required": true     
    },
    "enable": {
      "type": "Boolean",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "importorder": {
      "type": "hasMany",
      "model": "ImportOrder",
      "foreignKey": "supplierId"
    }
  },
  "acls": [],
  "methods": {}
}