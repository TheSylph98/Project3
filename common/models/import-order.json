{
  "name": "ImportOrder",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "mydb": {
      "table": "import_order"
    }
  },
  "strictObjectIDCoercion": true,
  "properties": {
    "supplierId": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "boolean",
      "required": false
    },
    "subtotal": {
      "type": "number",
      "required": false
    },
    "createdAt": {
      "type": "date",
      "required": false
    },
    "bookList": {
      "type": "array",
      "required": false
    }
  },
  "protected": ["id"],
  "relations": {
    "belongsToSupplier": {
      "type": "belongsTo",
      "model": "Supplier",
      "foreignKey": "supplierId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
