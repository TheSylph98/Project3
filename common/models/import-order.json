{
  "name": "ImportOrder",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "mydb": {
      "table": "import_order"
    }
  },
  "strictObjectIDCoercion": true,
  "properties": {
    "supplierId": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "boolean",
      "required": false
    },
    "subtotal": {
      "type": "number",
      "required": false
    },
    "createdAt": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "supplier": {
      "type": "belongsTo",
      "model": "Supplier",
      "foreignKey": "supplierId"
    },
    "books": {
      "type": "embedsMany",
      "model": "OrderDetail",
      "property": "bookList",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
