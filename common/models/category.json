{
  "name": "Category",
  "plural": "categorys",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true,
    "mydb": {
      "table": "category"
    }
  },
  "properties": {
    "uid": {
      "type": "String",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "name": {
      "type": "string",
      "required": true,
      "default": "unknow"
    }
  },
  "validations": [],
  "relations": {
    "books": {
      "type": "hasAndBelongsToMany",
      "model": "Book",
      "foreignKey": "categoryId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
