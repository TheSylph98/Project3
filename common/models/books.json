{
  "name": "Book",
  "plural": "books",
  "base": "Model",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true,
    "mydb": {
      "table": "books"
    }
  },
  "properties": {
    "uid": {
      "type": "String",
      "required": true,
      "index": { "unique": true }
    },
    "bookName": {
      "type": "string",
      "required": true   
    },
    "categoryId": {
      "type": ["number"],
      "required": true
    },
    "description": {
      "type": "string",
      "required": true     
    },
    "imgURL": {
      "type": ["string"],
      "required": true   
    },
    "publisher": {
      "type": "string",
      "required": false
    },
    "author": {
      "type": ["string"],
      "required": true,
      "default": "unknow"
    },
    "quantity": {
      "type": "number",
      "required": true
      
    },
    "price": {
      "type": "number",
      "required": true
    },
    "publishedAt": {
      "type": "Date",
      "required": false
      
    },
    "enable": {
      "type": "Boolean",
      "default": 1 
    }
  },
  "validations": [],
  "relations": {
    "category": {
      "type": "belongsTo",
      "model": "Category",
      "foreignKey": "categoryId"
    },
    "cart": {
      "type": "hasMany",
      "model": "Cart",
      "foreignKey": "bookId"
    },
    "order": {
      "type": "hasMany",
      "model": "Order",
      "foreignKey": "bookId"
    },
    "comment": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": "bookId"
    },
    "import-order": {
      "type": "hasMany",
      "model": "ImportOrder",
      "foreignKey": "bookId"
    }
  },
  "acls": [],
  "methods": {}
}