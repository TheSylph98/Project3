{
  "name": "Supplier",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true,
    "mydb": {
      "table": "supplier"
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": false
    },
    "phone": {
      "type": "string",
      "required": false
    },
    "createdAt": {
      "type": "Date",
      "required": false
    },
    "enable": {
      "type": "Boolean",
      "default": 1
    }
  },
  "protected" : ["id", "uid", "createdAt", "enable"],
  "relations": {
    "hasManyImportOder": {
      "type": "hasMany",
      "model": "ImportOrder",
      "foreignKey": "supplierId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
