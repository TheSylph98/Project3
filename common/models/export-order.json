{
  "name": "ExportOrder",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true,
    "mydb": {
      "table": "export_order"
    }
  },
  "properties": {
    "userId": {
      "type": "number",
      "required": true
    },
    "total": {
      "type": "number",
      "required": false
    },
    "promotionId": {
      "type": "number",
      "required": false
    },
    "createdAt": {
      "type": "date",
      "required": true
    },
    "finishAt": {
      "type": "date",
      "required": false
    },
    "paymentMethod": {
      "type": "string",
      "required": true
    },
    "addressShip": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "bStatus": {
      "type": "boolean",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "userId"
    },
    "promotion": {
      "type": "belongsTo",
      "model": "Promotion",
      "foreignKey": "promotionId"
    },
    "books": {
      "type": "embedsMany",
      "model": "OrderDetail",
      "property": "bookList",
      "options": {
        "validate": false,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
