{
  "name": "ExportOrder",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true,
    "mydb": {
      "table": "export_order"
    }
  },
  "properties": {
    "userId": {
      "type": "number",
      "required": true
    },
    "subtotal": {
      "type": "number",
      "required": false
    },
    "paymentMethod": {
      "type": "string",
      "required": true
    },
    "addressShip": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": true
    },
    "updatedAt": {
      "type": "date",
      "required": false
    }
  },
  "protected": ["id"],
  "relations": {
    "belongsToUser": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "userId"
    },
    "bookList": {
      "type": "embedsMany",
      "model": "OrderDetail",
      "property": "bookList",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
