{
  "name": "Cart",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true,
    "mydb": {
      "table": "cart"
    }
  },
  "properties": {
    "userId": {
      "type": "string",
      "required": true
    }
  },
  "relations": {
    "belongsToUser": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "userId"
    },
    "bookList": {
      "type": "embedsMany",
      "model": "OrderDetail",
      "property": "bookList",
      "options": {
        "forceId": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
