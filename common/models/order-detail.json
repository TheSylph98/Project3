{
    "name": "OrderDetail",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
      "validateUpsert": true,
      "strictObjectIDCoercion": true,
      "mydb": {
        "table": "order_detail"
      }
    },
    "properties": {
      "bookId": {
        "type": "string",
        "required": true
      },
      "quantity": {
        "type": "number",
        "required": true
      },
      "price": {
        "type": "number",
        "required": true
      }
    },
    "protected": ["id"],
    "relations": {
      "belongsToBook": {
        "type": "belongsTo",
        "model": "Book",
        "foreignKey": "bookId",
        "options": {
          "nestRemoting": true
        }
      }
    },
    "acls": [],
    "methods": {}
  }
  