{
  "name": "Book",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true,
    "mydb": {
      "table": "book"
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "categoryList": {
      "type": "array"
    },
    "description": {
      "type": "string",
      "required": false
    },
    "imgURL": {
      "type": "string",
      "required": false
    },
    "publisherId": {
      "type": "string",
      "required": true,
      "default": "unknow"
    },
    "authorId": {
      "type": "string"
    },
    "quantity": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "sellPrice": {
      "type": "number",
      "required": true
    },
    "publishedAt": {
      "type": "date",
      "required": false
    },
    "enable": {
      "type": "boolean",
      "default": 1
    }
  },
  "relations": {
    "hasManyComment": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": "bookId"
    },
    "belongsToAuthor": {
      "type": "belongsTo",
      "model": "Author",
      "foreignKey": "authorId"
    },
    "belongsToPublisher": {
      "type": "belongsTo",
      "model": "Publisher",
      "foreignKey": "publisherId"
    }
  },
  "foreignKeys": {
    "authorId": {
      "name": "book_author",
      "foreignKey": "authorId",
      "entityKey": "id",
      "entity": "Author"
    },
    "publisherId": {
      "name": "book_publisher",
      "foreignKey": "publisherId",
      "entityKey": "id",
      "entity": "Publisher"
    }
  },
  "acls": [],
  "methods": {}
}
