{
  "name": "Book",
  "plural": "books",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true,
    "mydb": {
      "table": "book"
    }
  },
  "properties": {
    "uid": {
      "type": "String",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "name": {
      "type": "string",
      "required": true
    },
    "categoryId": {
      "type": [
        "string"
      ],
      "required": true
    },
    "description": {
      "type": "string",
      "required": false
    },
    "imgURL": {
      "type": "string",
      "required": false
    },
    "publisher": {
      "type": "string",
      "required": true,
      "default": "unknow"
    },
    "author": {
      "type": [
        "string"
      ],
      "required": true,
      "default": "unknow"
    },
    "quantity": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "sellPrice": {
      "type": "number",
      "required": true
    },
    "publishedAt": {
      "type": "Date",
      "required": false
    },
    "enable": {
      "type": "Boolean",
      "default": 1
    }
  },
  "validations": [],
  "relations": {
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": "bookId"
    },
    "categories": {
      "type": "hasMany",
      "model": "Category",
      "foreignKey": ""
    },
    "orderDetail": {
      "type": "hasAndBelongsToMany",
      "model": "OrderDetail",
      "foreignKey": ""
    }
  
  },
  "acls": [],
  "methods": {}
}
